{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "enabledManagers": [
    "helm-values"
  ],
  "hostRules": [
    {
      "matchHost": "registry.enginsight.com",
      "hostType": "docker"
    }
  ],
  "packageRules": [
    {
      "matchManagers": [
        "helm-values"
      ],
      "matchPackagePatterns": [
        "registry.enginsight.com/enginsight/*"
      ]
    }
  ]
}