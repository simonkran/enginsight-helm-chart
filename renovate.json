{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "prHourlyLimit": 20,
  "enabledManagers": [
    "helm-values"
  ],
  "packageRules": [
    {
      "matchManagers": [
        "helm-values"
      ],
      "matchPackagePatterns": [
        "registry.enginsight.com/enginsight/*"
      ]
    }
  ]
}